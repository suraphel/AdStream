{
  "dashboard": {
    "id": null,
    "title": "EthioMarket - Business Metrics (ኢትዮማርኬት - የንግድ መለኪያዎች)",
    "tags": ["ethiomarket", "business", "listings"],
    "timezone": "browser",
    "refresh": "1m",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Total Listings | ጠቅላላ ዝርዝሮች",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(ethiomarket_listings_total)",
            "legendFormat": "Total Listings"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Active Listings | ንቁ ዝርዝሮች",
        "type": "stat",
        "targets": [
          {
            "expr": "ethiomarket_listings_total{status=\"active\"}",
            "legendFormat": "Active Listings"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 100 },
                { "color": "green", "value": 500 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 0 }
      },
      {
        "id": 3,
        "title": "User Registrations | የተጠቃሚ ምዝገባዎች",
        "type": "stat",
        "targets": [
          {
            "expr": "ethiomarket_users_total",
            "legendFormat": "Total Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 }
      },
      {
        "id": 4,
        "title": "Search Success Rate | የፍለጋ ስኬት መጠን",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(ethiomarket_search_requests_total{has_results=\"true\"}[1h]) / rate(ethiomarket_search_requests_total[1h])",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "red", "value": null },
                { "color": "yellow", "value": 0.5 },
                { "color": "green", "value": 0.8 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 }
      },
      {
        "id": 5,
        "title": "Listings by Category | በምድብ የተከፋፈሉ ዝርዝሮች",
        "type": "piechart",
        "targets": [
          {
            "expr": "ethiomarket_listings_total{status=\"active\"}",
            "legendFormat": "{{category}}"
          }
        ],
        "gridPos": { "h": 10, "w": 12, "x": 0, "y": 8 }
      },
      {
        "id": 6,
        "title": "Search Requests by Category | በምድብ የፍለጋ ጥያቄዎች",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ethiomarket_search_requests_total[5m])",
            "legendFormat": "{{category}}"
          }
        ],
        "yAxes": [
          {
            "label": "Searches/sec",
            "min": 0
          }
        ],
        "gridPos": { "h": 10, "w": 12, "x": 12, "y": 8 }
      },
      {
        "id": 7,
        "title": "Favorite Actions | የተወዳጅ እርምጃዎች",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ethiomarket_favorite_actions_total{action=\"add\"}[5m])",
            "legendFormat": "Favorites Added"
          },
          {
            "expr": "rate(ethiomarket_favorite_actions_total{action=\"remove\"}[5m])",
            "legendFormat": "Favorites Removed"
          }
        ],
        "yAxes": [
          {
            "label": "Actions/sec",
            "min": 0
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 }
      },
      {
        "id": 8,
        "title": "User Activity Heatmap | የተጠቃሚ እንቅስቃሴ ካርታ",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(ethiomarket_http_requests_total[1h])",
            "legendFormat": "Activity Level"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 }
      },
      {
        "id": 9,
        "title": "Top User Actions | ዋና ዋና የተጠቃሚ እርምጃዎች",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, rate(ethiomarket_http_requests_total[1h]))",
            "legendFormat": "{{route}} - {{method}}"
          }
        ],
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 26 }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "language",
          "type": "custom",
          "options": [
            { "text": "English", "value": "en", "selected": true },
            { "text": "አማርኛ", "value": "am", "selected": false }
          ]
        }
      ]
    },
    "annotations": {
      "list": []
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "links": [],
    "liveNow": false,
    "panels": [],
    "schemaVersion": 27,
    "style": "dark",
    "version": 1,
    "weekStart": ""
  }
}